<!DOCTYPE html>
<html>
<head>
  <title>HelseBackEnd</title>
  <%= stylesheet_link_tag 'blueprint/screen', :media => 'screen' %>
  <%= stylesheet_link_tag 'blueprint/print',  :media => 'print' %>
  <%= stylesheet_link_tag    "application", :media => "all" %>

  <%= stylesheet_link_tag    "backend", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= yield :javascript_includes %>
  <%= csrf_meta_tags %>
</head>
<body>
    <%= render :partial => 'shared/login'%>
  <div id="wrapper">
  <header id="main_header">
    <div id="main_nav">
      <h1>HelseApp</h1>
      <ul>
        <li><%= link_to "Hjem", :controller => "site" %></li>
        <li><%= link_to "Ansatte", :controller => "employee" %></li>
        <li><%= link_to "Pasienter", :controller => "patient" %></li>
        <li><%= link_to "Senter", :controller => "workplace" %></li>
        <li><%= link_to "Besok", :controller => "visit" %></li>
        <li><%= link_to "Oppgavemaler", :controller => "task_template" %></li>
      </ul>
    </div><!-- end main_nav -->
  </header>
  <%= render :partial => 'context' rescue render "shared/context" %>
  
  <div id="main_content">
    <%= render :partial => "shared/flash", :locals => {:flash => flash} %>

  <%= yield %>
  
  </div> <!-- end main_content -->
  <p id="clear"></p>
  </div><!-- end wrapper -->
</body>
</html>
