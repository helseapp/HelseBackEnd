<table>
  <tr>
    <th>ID</th>
    <th>Fornavn</th>
    <th>Etternavn</th>
    <th>Telefonnummer</th>
    <th>Systembruker</th>
  </tr>

<% @employees.each do |e| %>
<tr>
  <td><%= link_to e.id, :action => "show", :id => e.id %></td>
  <td><%= e.firstname %></td>
  <td><%= e.lastname %></td>
  <td><%= e.mobilephone %></td>
  <% #Hvis bruker er systembruker skriv: vis systembrukerprofil, ellers gjør til systembruker %>
  <td><%= link_to "Gjør til systembruker", :controller => "appuser", :action => "upgrade", :id => e.id %></td>
</tr> 
<% end %>

</table>

