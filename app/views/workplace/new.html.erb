<%= form_for @workplace, :url => "/workplace/create" do |w| %>


  <%= w.label :name %>:
  <%= w.text_field :name %><br />

  <%= w.label :address %>:
  <%= w.text_field :address %><br />
  
  <%= w.label :postnumber %>:
  <%= w.text_field :postnumber %><br />
  
  <%= w.label :postplace %>:
  <%= w.text_field :postplace %><br />
  
  <%= w.label :telephone %>:
  <%= w.text_field :telephone %><br />
  
  <%= w.submit %>
<% end %>
